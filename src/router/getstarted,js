import Base from '@/views/Layout/Base.vue';

const routes = [
  {
    path: '',
    component: Base,
    meta: { requiresAuth: true },
    children: [
      {
        path: '/get-started',
        name: 'GetStarted',
        component: () => import('@/views/GetStarted.vue'),
      },
      {
        path: '/forgot-password',
        name: 'ForgotPassword',
        component: () => import('@/views/ForgotPassword.vue'),
      },
    ],
  },
];


export default routes;
